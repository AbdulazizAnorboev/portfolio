{
  "compilerOptions": {
    "target": "ES2017",
      "paths": {
      "sanity/desk": ["./node_modules/sanity/lib/exports/desk"]
    },
     "baseUrl": ".",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "Preserve",
    "moduleDetection": "force",
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["**/*.ts", "**/*.tsx", "sanity/**/*", "deskstructure.js", "structure.ts"],
  "exclude": ["node_modules"]
}


